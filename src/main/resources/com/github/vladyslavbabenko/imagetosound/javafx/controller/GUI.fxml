<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.*?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.github.vladyslavbabenko.imagetosound.javafx.controller.FXController">
    <children>
        <HBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0">
            <children>
                <ImageView cache="true" fitHeight="370.0" fitWidth="370.0" pickOnBounds="true" preserveRatio="true">
                    <HBox.margin>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
                    </HBox.margin>
                </ImageView>
                <Line endX="-100.0" endY="400.0" startX="-100.0"/>
                <VBox alignment="CENTER" prefHeight="400.0" prefWidth="200.0" spacing="5.0">
                    <children>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <Button alignment="CENTER" contentDisplay="CENTER" minHeight="-Infinity"
                                        minWidth="-Infinity" mnemonicParsing="false" prefWidth="80.0"
                                        text="Upload Image" textAlignment="CENTER" wrapText="true">
                                    <font>
                                        <Font name="Calibri Bold" size="14.0"/>
                                    </font>
                                </Button>
                            </children>
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0" spacing="10.0">
                                    <children>
                                        <Label text="Track Length" textAlignment="CENTER">
                                            <font>
                                                <Font name="System Bold" size="14.0"/>
                                            </font>
                                        </Label>
                                        <Slider blockIncrement="5.0" majorTickUnit="5.0" max="30.0" maxWidth="-Infinity"
                                                min="5.0" minWidth="-Infinity" prefWidth="160.0" showTickLabels="true"
                                                showTickMarks="true"/>
                                    </children>
                                </VBox>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                    <children>
                                        <Label text="Image" textAlignment="CENTER">
                                            <font>
                                                <Font name="System Bold" size="10.0"/>
                                            </font>
                                        </Label>
                                        <Label alignment="CENTER" prefHeight="49.0" prefWidth="28.0" text="X"
                                               textAlignment="CENTER" textFill="RED">
                                            <font>
                                                <Font name="System Bold" size="24.0"/>
                                            </font>
                                        </Label>
                                    </children>
                                </VBox>
                                <Button contentDisplay="CENTER" disable="true" minHeight="-Infinity"
                                        minWidth="-Infinity" mnemonicParsing="false" prefWidth="80.0"
                                        text="Convert Image" textAlignment="CENTER" wrapText="true">
                                    <font>
                                        <Font name="Calibri Bold" size="14.0"/>
                                    </font>
                                </Button>
                                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0"/>
                            </children>
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <Button contentDisplay="CENTER" minHeight="-Infinity" minWidth="-Infinity"
                                        mnemonicParsing="false" prefWidth="80.0" text="Upload Audio"
                                        textAlignment="CENTER" wrapText="true">
                                    <font>
                                        <Font name="Calibri Bold" size="14.0"/>
                                    </font>
                                </Button>
                            </children>
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                    <children>
                                        <Label text="Image" textAlignment="CENTER">
                                            <font>
                                                <Font name="System Bold" size="10.0"/>
                                            </font>
                                        </Label>
                                        <Label alignment="CENTER" prefHeight="49.0" prefWidth="28.0" text="X"
                                               textAlignment="CENTER" textFill="RED">
                                            <font>
                                                <Font name="System Bold" size="24.0"/>
                                            </font>
                                        </Label>
                                    </children>
                                </VBox>
                                <Button alignment="CENTER" contentDisplay="CENTER" disable="true" minHeight="-Infinity"
                                        minWidth="-Infinity" mnemonicParsing="false" prefWidth="80.0" text="MASH-up"
                                        textAlignment="CENTER">
                                    <font>
                                        <Font name="Calibri Bold" size="14.0"/>
                                    </font>
                                </Button>
                                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                    <children>
                                        <Label text="Audio" textAlignment="CENTER">
                                            <font>
                                                <Font name="System Bold" size="10.0"/>
                                            </font>
                                        </Label>
                                        <Label alignment="CENTER" prefHeight="49.0" prefWidth="28.0" text="X"
                                               textAlignment="CENTER" textFill="RED">
                                            <font>
                                                <Font name="System Bold" size="24.0"/>
                                            </font>
                                        </Label>
                                    </children>
                                </VBox>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0" spacing="10.0">
                                    <children>
                                        <Label text="Relative Brightness" textAlignment="CENTER">
                                            <font>
                                                <Font name="System Bold" size="14.0"/>
                                            </font>
                                        </Label>
                                        <Slider majorTickUnit="5.0" max="10.0" maxWidth="-Infinity" min="-10.0"
                                                minWidth="-Infinity" minorTickCount="5" prefWidth="140.0"
                                                showTickLabels="true"/>
                                    </children>
                                </VBox>
                            </children>
                        </HBox>
                    </children>
                    <padding>
                        <Insets bottom="10.0" top="10.0"/>
                    </padding>
                </VBox>
            </children>
        </HBox>
    </children>
</AnchorPane>
